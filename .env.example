# Environment Variable Template
# Copy this file to .env and fill in your actual values for local development.
# For production/staging, these variables should be set directly in the environment or through secrets management.

# Azure SQL Database connection
SQL_CONNECTION_STRING="Server=<your-server-name>.database.windows.net;Database=<your-database-name>;User Id=<your-user-id>;Password=<your-password>;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"

# Application Insights
APPLICATIONINSIGHTS_CONNECTION_STRING="InstrumentationKey=<your-instrumentation-key>;IngestionEndpoint=<your-ingestion-endpoint>"

# Instance name (e.g., local-dev, staging, production)
INSTANCE_NAME=""
